// Assignment by Raj Patel
#include <stdio.h>

int main() {
    int A[3][3] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };
    int B[3][3] = {
        {9, 8, 7},
        {6, 5, 4},
        {3, 2, 1}
    };
    int sum[3][3], diff[3][3], prod[3][3];
    int i, j, k;

    // Matrix Addition
    for (i = 0; i < 3; i++)
        for (j = 0; j < 3; j++)
            sum[i][j] = A[i][j] + B[i][j];

    // Matrix Subtraction
    for (i = 0; i < 3; i++)
        for (j = 0; j < 3; j++)
            diff[i][j] = A[i][j] - B[i][j];

    // Matrix Multiplication
    for (i = 0; i < 3; i++)
        for (j = 0; j < 3; j++) {
            prod[i][j] = 0;
            for (k = 0; k < 3; k++)
                prod[i][j] += A[i][k] * B[k][j];
        }

    // Display Results
    printf("Matrix Addition:\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++)
            printf("%4d", sum[i][j]);
        printf("\n");
    }

    printf("\nMatrix Subtraction:\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++)
            printf("%4d", diff[i][j]);
        printf("\n");
    }

    printf("\nMatrix Multiplication:\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++)
            printf("%4d", prod[i][j]);
        printf("\n");
    }

    return 0;
}
